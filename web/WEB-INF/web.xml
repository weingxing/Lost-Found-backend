    <servlet>
        <servlet-name>ChangeStatus</servlet-name>
        <servlet-class>service.ChangeStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangeStatus</servlet-name>
        <url-pattern>/changeStatus</url-pattern>
    </servlet-mapping>
</web-app>